{
  "models": {
    "azure": {
      "name": "Azure OpenAI",
      "api_key_env": "AZURE_OPENAI_API_KEY",
      "api_base_env": "AZURE_OPENAI_ENDPOINT",
      "api_version": "2024-08-01-preview",
      "deployment_name": "gpt-4o-mini",
      "model_name": "gpt-4o-mini",
      "embedding_deployment": "text-embedding-3-small"
    },
    "anthropic": {
      "name": "Anthropic Claude",
      "api_key_env": "ANTHROPIC_API_KEY",
      "model_name": "claude-3-7-sonnet-20250219"
    },
    "llama": {
      "name": "Meta LLaMA",
      "model_path": "models/llama",
      "model_name": "llama-7b"
    },
    "gemini": {
      "name": "Google Gemini-2.0-Flash",
      "api_key_env": "GOOGLE_API_KEY",
      "model_name": "gemini-2.0-flash"
    },
    "qwen": {
      "name": "Qwen",
      "api_key_env": "QWEN_API_KEY",
      "model_name": "qwen-14b"
    },
    "deepseek": {
      "name": "DeepSeek",
      "api_key_env": "DEEPSEEK_API_KEY",
      "model_name": "deepseek-coder"
    }
  },
  "default_model": "azure",
  "max_tokens": 8000,
  "system_prompt": "You are a helpful AI research assistant. Your goal is to help researchers write new papers or expand work-in-progress papers based on the provided documents and instructions.",
  "preserve_neo4j_data": true,
  "download_neo4j_if_missing": true
}